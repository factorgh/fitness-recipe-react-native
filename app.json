{
  "expo": {
    "name": "fitness recipe",
    "slug": "fitness-recipe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/launch_icon.png",
    "scheme": "fitnessrecipe",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/launch_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/final_app.png",
        "backgroundColor": "#FFBF46"
      },
      "package": "com.facctorgh.fitnessrecipe"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5dae5ed5-8eef-4165-9d4c-55a4028c5003"
      }
    }
  }
}
